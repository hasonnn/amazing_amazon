<h1>Contact us</h1>
<%= form_tag(contacts_path) do %> 
    <div>
        <%= label_tag :name%>
        <%= text_field_tag :name %> 
    </div>
    <div>
        <%= label_tag :email%>
        <%= text_field_tag :email %> 
    </div>
    <div>
        <%= label_tag :message%>
        <%= text_field_tag :message %> 
    </div>
    <br>
    <%= submit_tag 'Contact Us'%>
<% end %>